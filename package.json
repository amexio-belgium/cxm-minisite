{
  "name": "amexiocxmwebsite",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "generate-translations": "node src/scripts/generateTranslations.js",
    "fix-sitemap": "node src/scripts/fixSitemap.js",
    "font-script": "node src/scripts/fontscript.js",
    "pages-script": "node src/scripts/pagesScript.js",
    "rss-script": "node src/scripts/generateRssFiles.js",
    "prebuild": "node src/scripts/prebuild.js && npm run pages-script && npm run font-script && npm run generate-translations",
    "postbuild": "node src/scripts/postbuild.js && npm run rss-script && npm run fix-sitemap",
    "dev": "npm run font-script && astro dev",
    "build": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && astro check && astro build",
    "preview": "npm run prebuild && astro preview",
    "astro": "astro",
    "postinstall": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide"
  },
  "overrides": {
    "vite": {
      "rollup": "npm:@rollup/wasm-node"
    }
  },
  "dependencies": {
    "@astro-community/astro-embed-youtube": "^0.5.6",
    "@astrojs/check": "^0.9.4",
    "@astrojs/netlify": "^5.5.4",
    "@astrojs/react": "^3.6.2",
    "@astrojs/rss": "^4.0.9",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/tailwind": "^5.1.2",
    "@inlang/paraglide-astro": "^0.2.2",
    "@portabletext/react": "^3.1.0",
    "@portabletext/to-html": "^2.0.13",
    "@sanity/astro": "^3.1.6",
    "@sanity/cli": "^3.64.1",
    "@sanity/client": "^6.22.2",
    "@sanity/image-url": "^1.1.0",
    "@sanity/insert-menu": "^1.0.11",
    "@sanity/visual-editing": "^2.7.1",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "astro": "^4.16.13",
    "astro-embed": "^0.9.0",
    "astro-portabletext": "^0.10.1",
    "build": "^0.1.4",
    "dotenv": "^16.4.5",
    "feed": "^4.2.2",
    "fs-extra": "^11.2.0",
    "groq": "^3.64.1",
    "lodash-es": "^4.17.21",
    "npm": "^10.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rss": "^1.2.2",
    "run": "^1.5.0",
    "sanity": "^3.64.1",
    "shelljs": "^0.8.5",
    "tailwindcss": "^3.4.15",
    "tailwindcss-animated": "^1.1.2",
    "typescript": "^5.6.3",
    "uuid": "^10.0.0",
    "vitest": "^2.1.5",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@inlang/paraglide-js": "1.11.3",
    "@sanity/export": "^3.41.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/rss": "^0.0.32",
    "@types/xml2js": "^0.4.14",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tsx": "^4.19.1",
    "vite": "^5.4.11"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.5"
  }
}
