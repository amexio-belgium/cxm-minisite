{
  "name": "amexiocxmwebsite",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "generate-translations": "node src/scripts/generateTranslations.js",
    "fix-sitemap": "node src/scripts/fixSitemap.js",
    "font-script": "node src/scripts/fontscript.js",
    "prebuild": "npm run font-script && npm run generate-translations",
    "postbuild": "node src/scripts/postbuild.js && node src/scripts/generateRssFiles.js",
    "dev": "npm run font-script && astro dev",
    "build": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && astro check && astro build",
    "preview": "npm run prebuild && astro preview",
    "astro": "astro",
    "postinstall": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide"
  },
  "dependencies": {
    "@astro-community/astro-embed-youtube": "^0.5.5",
    "@astrojs/check": "^0.9.4",
    "@astrojs/netlify": "^5.5.3",
    "@astrojs/react": "^3.6.2",
    "@astrojs/rss": "^4.0.8",
    "@astrojs/sitemap": "^3.2.0",
    "@astrojs/tailwind": "^5.1.2",
    "@inlang/paraglide-astro": "^0.2.2",
    "@otterstack/sanity-img-astro": "^0.2.6",
    "@portabletext/react": "^3.1.0",
    "@portabletext/to-html": "^2.0.13",
    "@sanity/astro": "^3.1.6",
    "@sanity/client": "^6.21.3",
    "@sanity/image-url": "^1.0.2",
    "@sanity/visual-editing": "^2.1.10",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "astro": "^4.16.0",
    "astro-embed": "^0.7.4",
    "astro-portabletext": "^0.10.0",
    "dotenv": "^16.4.5",
    "feed": "^4.2.2",
    "fs-extra": "^11.2.0",
    "groq": "^3.60.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rss": "^1.2.2",
    "sanity": "^3.60.0",
    "tailwindcss": "^3.4.13",
    "tailwindcss-animated": "^1.1.2",
    "typescript": "^5.6.3",
    "uuid": "^10.0.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@inlang/paraglide-js": "1.11.3",
    "@tailwindcss/typography": "^0.5.15",
    "@types/fs-extra": "^11.0.4",
    "@types/rss": "^0.0.32",
    "@types/xml2js": "^0.4.14",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tsx": "^4.19.1",
    "vite": "^5.4.8"
  }
}
