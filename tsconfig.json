{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "strict": false, // see ticket CW-117 to fix this
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@assets/*": ["src/assets/*"],
      "@lib/*": ["src/lib/*"],
      "@api/*": ["src/api/*"],
      "@sanity/*": ["src/sanity/*"],
      "@src/*": ["src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "@builder.io/qwik"
  }
}
